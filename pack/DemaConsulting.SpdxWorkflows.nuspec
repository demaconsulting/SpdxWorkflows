        <id>DemaConsulting.SpdxWorkflows</id>
        <version>0.0.0</version>
        <title>SPDX Workflows</title>
        <authors>Dema Consulting</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
